<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <system:String x:Key="lang">English</system:String>
    <system:String x:Key="minimized">Application has been minimized</system:String>
    
    <system:String x:Key="username_label">Username</system:String>
    <system:String x:Key="password_label">Password</system:String>
    <system:String x:Key="login_label">Log in</system:String>

    <system:String x:Key="folders_label">My folders</system:String>

</ResourceDictionary>